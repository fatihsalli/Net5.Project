@model Order
@{
    ViewData["Title"] = "CompleteCard";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="container mt-2">
    <div class="col-md-6 mx-auto">
        <div class="card">
            <p>
                <span>@Model.OrderNumber numaralı siparişiniz oluşturuldu. Detaylı bilgilere epostanız üzerinden ulaşabilirsiniz.</span>
            </p>
        </div>
    </div>
</div>

